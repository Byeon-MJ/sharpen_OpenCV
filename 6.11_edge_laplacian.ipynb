   "source": [
    "import cv2\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "634bbca2",
   "metadata": {},
   "outputs": [],
   "source": [
    "img = cv2.imread('./img/sudoku.jpg')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "e4db8455",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Apply Laplacian filter\n",
    "edge = cv2.Laplacian(img, -1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "7683677e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Output Result\n",
    "merged = np.hstack((img, edge))\n",
    "cv2.imshow('Laplacian', merged)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  },
